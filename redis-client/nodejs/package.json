{
  "name": "redis-nodejs",
  "version": "1.0.0",
  "description": "Learn redis client with nodejs",
  "main": "index.js",
  "scripts": {
    "start": "ts-node-dev -T ./index.ts",
    "string": "npx ts-node-dev -T ./src/string.ts",
    "hash": "npx ts-node-dev -T ./src/hash.ts",
    "run-pipeline": "npx ts-node-dev -T ./src/pipeline.ts",
    "set": "npx ts-node-dev -T ./src/set.ts",
    "sorted-set": "npx ts-node-dev -T ./src/sorted-set.ts",
    "sort": "npx ts-node-dev -T ./src/sort.ts",
    "hyperloglog": "npx ts-node-dev -T ./src/hyperloglog.ts",
    "list": "npx ts-node-dev -T ./src/list.ts",
    "transactions": "npx ts-node-dev -T ./src/transactions.ts",
    "concurrency": "npx ts-node-dev -T src/Concurrency/bid.ts",
    "search": "npx ts-node-dev -T src/RediSearch/search.ts",
    "stream-publisher": "npx ts-node-dev -T src/Stream/publisher.ts",
    "stream-subscriber": "npx ts-node-dev -T src/Stream/subscriber.ts"
  },
  "keywords": [
    "redis",
    "redis-client",
    "redis-nodejs"
  ],
  "author": "Angga Ari Wijaya",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@types/luxon": "^3.4.2",
    "dotenv": "^16.4.5",
    "luxon": "^3.4.4",
    "redis": "^4.6.13",
    "redis-cli": "^2.1.2",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.8"
  },
  "devDependencies": {
    "tslib": "^2.6.2",
    "typescript": "^5.4.3"
  }
}
